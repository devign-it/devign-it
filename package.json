        "gatsby-source-filesystem": "^2.1.39",
        "gatsby-transformer-remark": "^2.6.37",
        "gatsby-transformer-sharp": "^2.3.5",
        "gsap": "^3.0.1",
        "jquery": "^3.4.1",
        "lodash": "^4.17.15",
        "node-sass": "^4.13.0",
        "npm-check-updates": "^3.2.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-helmet": "^5.2.1",
        "react-intersection-observer": "^8.25.1",
        "sharp": "^0.23.3",
        "styled-components": "^4.4.1",
